<%- include partials/header -%>
<div class="post-page">
    <h1><%= title %> </h1>
    <p><%= content %></p>
</div>

<div class="comment-bar">
    <div class="like-bar">
        <span><i class="far fa-heart fa-2x"></i></span>
        <span><i class="far fa-eye fa-2x"></i></span>
        <span><i class="fas fa-share-square fa-2x"></i></span>
        <span><i class="far fa-bookmark fa-2x"></i></span>
    </div>
    <input type="text" name="post-comment" id="post-comment" placeholder="comment bar :)">
</div>
</div>


<div class="footer-padding"></div>
  <div class="footer">
    <p>Made with ❤️ by 𝖓𝖔𝖉𝖊 𝖇𝖚𝖘 team</p>
  </div>
</div>
