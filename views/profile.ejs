<%- include('partials/header') %>
<link rel="stylesheet" href="css/settings.css">  
   
   

</head>

<body>
  <div class="container">


	<h1 class="header">To Upload Image on mongoDB</h1>
	<hr>
	<div class="card"> 
		<form action="/settings" method="POST" enctype="multipart/form-data">
			<div>
			
			
			</div>
			
			<div>
			
				<input type="file" id="image"
					name="image" value="" required>
			</div>
			<div>
				<button type="submit">Submit</button>
			</div>
		</form>
	</div>

	<hr>

	<h1>my profile</h1>
	<div>
		
		<div class="card">
			<div class="top">
                <p class="heading"><%= username %></p>
			 <img src="data:image/png;base64,"
        class="circle-img">
				<div class="bottom">
                    <div>
                        <p class="info">hello</p>
                    </div>
					
                    </div>
			 </div>
		</div>

    <div>
      <% items.forEach(function(image) { %>
      <div class="card">
        <div class="top">
                  <p class="heading"><%= image.name %></p>
         <img src="data:image/<%=image.img.contentType%>;base64,
            <%=image.img.data.toString('base64')%>" class="circle-img" style="width: 120px;">
          <div class="bottom">
                      <div class="note">
                          <p class="info"><%= image.desc %></p>
                      </div>
            
                      </div>
         </div>
      </div>
  
      <% }) %>
    </div>
	
	</div>
</div>
</body>

</html>
